@model List<ECOMMERCE.CORE.EntityPais>
@{
    ViewBag.Title = "Paises List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>              
            </tr>
        </thead>
        <tbody>
            @foreach (var pais in Model)
            {
                <tr>
                    <td>@pais.ididentifier_i</td>
                    <td>@pais.name_nv</td>

                    <td class="text-center"><a href="#" class="btn btn-primary">View</a></td>
                    <td class="text-center"><a href="#" class="btn btn-primary">Edit</a></td>
                    <td class="text-center"><a href="#" class="btn btn-danger">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>